{
  "name": "tweedle",
  "version": "0.0.0",
  "description": "Open API for downloading all media of a Twitter profile.",
  "main": "./src/index.js",
  "scripts": {
    "postinstall": "node ./src/scripts/create-dotenv.js && node ./src/scripts/create-media-dir.js",
    "start": "node ./src/index.js",
    "dev": "nodemon --verbose --ext js --watch ./src",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matze-kalle/tweedle.git"
  },
  "author": "Luca Feigl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matze-kalle/tweedle/issues"
  },
  "homepage": "https://github.com/matze-kalle/tweedle#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "twitter-lite": "^0.9.4"
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.4",
    "nodemon": "^2.0.2"
  },
  "husky": {
    "hooks": {
